<Window x:Class="WrapPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WrapPanel"
        mc:Ignorable="d"
        Title="WrapPanel" Height="450" Width="800">
    <Grid>
        <WrapPanel>
            <Label Name="label_CityId" Content="This is a WrapPanel Test:"/>
            <TextBox Name="textBlock_CityID" MinWidth ="150" />
            <Label Name="label_CityName" Content="This is a WrapPanel Test:"/>
            <TextBox Name="textBlock_CityName" MinWidth ="150" />
        </WrapPanel>
    </Grid>
</Window>
